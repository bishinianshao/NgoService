
<view>
  <view>
    <view class="usercontatiner">
      <view>
      <van-icon size='200rpx' color='#DC6D6D'  name="manager" bindclick="toRoleAudit" />
      <view class="getrequire">角色审核</view>
      </view>
      <view>
      <van-icon size='200rpx' color='#DC6D6D'  name="coupon" bindclick="toRegistrationAudit" />
      <view class="getrequire">注册审核</view>
      </view> 
      <view style="padding-left: 45rpx">
      <van-icon size='200rpx' color='#DC6D6D'  name="friends" bindclick="toRequireAudit" />
      <view class="getrequire">需求审核</view>
      </view>
    </view>    
      <view class="usercontatiner">
      <view>
        <van-icon size='200rpx' color='#DC6D6D' name="like" bindclick="toApplyAudit"/>
        <view class="getrequire">探访审核</view>
      </view>
      <view>
        <van-icon size='200rpx' color='#DC6D6D' name="invition" bindclick="navFollowAudit"/>
        <view class="getrequire">后期跟进审核</view>
      </view>
      <view>
        <van-icon size='200rpx' color='#DC6D6D' name="label" bindclick="navUserAudit"/>
        <view class="getrequire">人员管理</view>
      </view>
      </view>   
  </view>
  <view class="btn">
    <button class="button" bindtap='toUser'>用户</button>
  </view>
  <van-toast id="van-toast" />
  <view>
      <van-tabbar active="{{ active }}" active-color="#07c160" bind:change="onChange">
          <van-tabbar-item name="home" icon="home-o">首页</van-tabbar-item>
          <van-tabbar-item name="search" icon="search">通知</van-tabbar-item>
          <van-tabbar-item name="mine" icon="friends-o">我的</van-tabbar-item>
      </van-tabbar> 
  </view>
</view>
